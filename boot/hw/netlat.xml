<config>
    <mods>
        <mod name="fs" file="default.img" />
    </mods>
    <kernel args="kernel" />
    <dom>
        <app args="root">
            <dom>
                <app args="m3fs mem" daemon="1">
                    <serv name="m3fs" />
                    <mod name="fs" />
                </app>
            </dom>
            <dom>
                <app args="pager maxcli=2" usermem="768M">
                    <sess name="m3fs" />
                    <mod name="fs" perm="r" />
                    <mod name="tilemux" perm="r" />
                    <tiles type="perf+nic" count="1" />
                    <tiles type="perf" count="1" />
                    <dom tile="perf+nic">
                        <app args="/sbin/net net 192.168.41.11" daemon="1">
                            <serv name="net" />
                        </app>
                    </dom>
                    <dom tile="perf">
                        <app args="/bin/netlat 192.168.41.10 1338">
                            <sess name="net" args="bufs=1M socks=1" />
                        </app>
                    </dom>
                </app>
            </dom>
        </app>
    </dom>
</config>
