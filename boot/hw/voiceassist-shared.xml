<config>
    <mods>
        <mod name="fs" file="bench.img" />
    </mods>
    <kernel args="kernel" />
    <dom>
        <app args="root">
            <dom>
                <app args="m3fs mem" daemon="1">
                    <serv name="m3fs" />
                    <mod name="fs" />
                </app>
            </dom>
            <dom tile="perf+nic">
                <app args="pager maxcli=3">
                    <sess name="m3fs" />
                    <mod name="fs" perm="r" />
                    <mod name="tilemux" perm="r" />
                    <tiles type="effi" count="1" />
                    <app args="/sbin/net -m 1 net 192.168.41.11" daemon="1">
                        <serv name="net" />
                    </app>
                    <dom tile="effi">
                        <app args="/bin/vamic /testcase1.wav" daemon="1">
                            <mount fs="m3fs" path="/" />
                            <serv name="vamic" />
                        </app>
                    </dom>
                    <app args="/bin/vasnd -w 2 -r 8 udp 192.168.41.10 1337">
                        <sess name="net" args="bufs=1M socks=1" />
                        <sess name="vamic" />
                    </app>
                </app>
            </dom>
        </app>
    </dom>
</config>
