<config>
    <env>M3_GEM5_CFG=config/rot.py M3_GEM5_O3_CORES=1 M3_GEM5_MINOR_CORES=3</env>
    <rot layers="brom,blau.bin,rosa.bin" />
    <kernel args="kernel" />
    <dom>
        <app args="root">
            <dom tile="core+rot">
                <app args="rots" daemon="1">
                    <serv name="rot" />
                    <mod name="rot-certificate.json" perm="r" />
                </app>
            </dom>
            <dom tile="effi|core">
                <app args="raser">
                    <sess name="rot" args="rotc" />
                </app>
            </dom>
        </app>
    </dom>
</config>
